<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AdminApp" namespace="AdminApp.Dominio">
	<!--  Mapeo de la tabla 'produktua'  -->
	<class name="Produktua, AdminApp" table="produktua">
		<!--  Mapeo de la clave primaria  -->
		<id name="Id" column="id" type="int">
			<generator class="identity"/>
		</id>
		<!--  Mapeo de las propiedades  -->
		<property name="Izena" column="izena" type="string"/>
		<property name="Deskribapena" column="deskribapena" type="string"/>
		<property name="Prezioa" column="prezioa" type="float"/>
		<property name="ErosketaPrezioa" column="erosketaPrezioa" type="float"/>
		<property name="Kantitatea" column="kantitatea" type="int"/>
		<property name="KantitateMin" column="kantitateaMinimoa" type="int"/>
		<property name="Mota" column="mota" type="int"/>
	</class>
</hibernate-mapping>
